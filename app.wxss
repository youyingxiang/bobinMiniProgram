/**app.wxss**/
page {
  font-family: -apple-system, BlinkMacSystemFont, 'Helvetica Neue', Helvetica, Segoe UI, Arial, Roboto, 'PingFang SC', 'Hiragino Sans GB', 'Microsoft Yahei', sans-serif;
  background-color: #f0f3f6;
  --tabbar-item-active-color: #3e57a7;
  --button-primary-border-color: #3e57a7;
  --button-primary-background-color: #3e57a7;
}
.van-tabbar-item {
  --tabbar-item-active-color: #3e57a7;
  --button-primary-border-color: #3e57a7;
  --button-primary-background-color: #3e57a7;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.add-house {
  /* margin-top: 10px; */
  background-color: #f0f3f6 !important;
}

.empty-img{
  width: 100%;
  height: 100%;
}
.empty-view{
  width: 160px;
  height: 160px;
  display: block;
}
.empty{
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  flex-direction: column;
  -webkit-box-align: center;
  align-items: center;
  -webkit-box-pack: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 32px 0;
}
.vempty-description{
  margin-top: 16px;
  padding: 0 60px;
  color: #969799;
  font-size: 14px;
  line-height: 20px;
}
.button-tip {
  text-align: center;
  color: #999;
  font-size: 0.857142857em;
  margin: 10px auto;
  display: block;

}

.button-tip .highlight {
  color: red;
}
