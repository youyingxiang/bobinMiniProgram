/* pages/reservelist/index.wxss */
.house-list{
    margin-top: 10px;
}
.empty-img{
    width: 100%;
    height: 100%;
}
.empty-view{
    width: 160px;
    height: 160px;
    display: block;
}
.empty{
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    box-sizing: border-box;
    padding: 32px 0;
}
.vempty-description{
    margin-top: 16px;
    padding: 0 60px;
    color: #969799;
    font-size: 14px;
    line-height: 20px;
}